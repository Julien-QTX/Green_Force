{% extends 'base.html.twig' %}

{% block title %}User index{% endblock %}

{% block body %}
    <h1>User index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Entreprise</th>
                <th>Responsable</th>
                <th>Email</th>
                <th>Telephone</th>
                <th>Mdp</th>
                <th>Confirmer Mdp</th>
                <th>AdresseL</th>
                <th>VilleL</th>
                <th>CpL</th>
                <th>AdresseF</th>
                <th>VilleF</th>
                <th>CpF</th>
                <th>NTVA</th>
                <th>NSIRET</th>
                <th>NKBIS</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for user in users %}
            <tr>
                <td>{{ user.id }}</td>
                <td>{{ user.entreprise }}</td>
                <td>{{ user.responsable }}</td>
                <td>{{ user.email }}</td>
                <td>{{ user.telephone }}</td>
                <td>{{ user.mdp }}</td>
                <td>{{ user.confirm_mdp }}</td>
                <td>{{ user.adresseL }}</td>
                <td>{{ user.villeL }}</td>
                <td>{{ user.CpL }}</td>
                <td>{{ user.adresseF }}</td>
                <td>{{ user.villeF }}</td>
                <td>{{ user.CpF }}</td>
                <td>{{ user.NTVA }}</td>
                <td>{{ user.NSIRET }}</td>
                <td>{{ user.NKBIS }}</td>
                <td>
                    <a href="{{ path('user_show', {'id': user.id}) }}">show</a>
                    <a href="{{ path('user_edit', {'id': user.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="16">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('user_new') }}">Create new</a>
{% endblock %}
